{
    "name": "@9elt/miniframe",
    "version": "0.13.0",
    "description": "A better version of document.createElement with support for JSX and minimal state management in 300 LOC",
    "main": "./dist/esm/index.js",
    "types": "./index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/esm/index.js",
            "require": "./dist/cjs/index.js",
            "types": "./index.d.ts"
        },
        "./jsx-runtime": {
            "import": "./dist/esm/jsx-runtime.js",
            "require": "./dist/cjs/jsx-runtime.js",
            "types": "./jsx-runtime.d.ts"
        },
        "./jsx-dev-runtime": {
            "import": "./dist/esm/jsx-runtime.js",
            "require": "./dist/cjs/jsx-runtime.js",
            "types": "./jsx-runtime.d.ts"
        },
        "./polyfill": {
            "import": "./dist/esm/polyfill.js",
            "require": "./dist/cjs/polyfill.js",
            "types": "./polyfill.d.ts"
        }
    },
    "scripts": {
        "test": "bun test && tsc",
        "build": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json && echo '{\"type\":\"module\"}'>dist/esm/package.json",
        "generate:types": "bun scripts/generate.types.ts",
        "pub": "npm run build && npm run test && npm publish"
    },
    "author": "Lorenzo Cicuttin",
    "license": "MIT",
    "keywords": [
        "state",
        "state-management",
        "createElement",
        "createNode",
        "jsx"
    ],
    "readme": "./README.md",
    "homepage": "https://github.com/9elt/miniframe#readme",
    "repository": "github:9elt/miniframe",
    "devDependencies": {
        "@types/bun": "^1.2.20",
        "@types/jsdom": "^21.1.7",
        "jsdom": "^22.1.0"
    }
}
