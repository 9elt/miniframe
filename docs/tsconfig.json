{
    "include": [
        "./src/**/*"
    ],
    "compilerOptions": {
        "target": "es2021",
        "module": "esnext",
        "moduleResolution": "bundler",
        "skipLibCheck": true,
        "strictNullChecks": true,
        "noErrorTruncation": true,
        "noEmit": true,
        "jsx": "react-jsx",
        "jsxImportSource": "@9elt/miniframe"
    }
}
