{
    "scripts": {
        "build": "NODE_ENV=production bun build ./src/index.tsx --outdir .",
        "build:dev": "NODE_ENV=production bun build ./src/index.tsx --outdir . --watch",
        "serve:dev": "bun --port 3000 ./index.html",
        "dev": "run-p build:dev serve:dev",
        "generate:snippets": "bun scripts/snippets.ts",
        "generate:version": "bun scripts/version.ts",
        "generate": "run-p generate:snippets generate:version"
    },
    "dependencies": {
        "@9elt/miniframe": "^0.16.0"
    },
    "devDependencies": {
        "@types/bun": "^1.2.20",
        "@types/prismjs": "^1.26.5",
        "highlight.js": "^11.11.1",
        "npm-run-all": "^4.1.5",
        "prismjs": "^1.30.0",
        "typescript": "^5.9.2"
    }
}
